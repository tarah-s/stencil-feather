{
  "name": "stencil-feather",
  "version": "1.0.2",
  "description": "Feather Icon web component using stencil.js for Vue, Angular & React to use icon as single icon or as a sprite",
  "repository": {
    "url": "https://github.com/tarah-s/stencil-feather"
  },
  "keywords": [
    "stencil",
    "feather",
    "web components",
    "icons",
    "vue",
    "angular",
    "react"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-feather/stencil-feather.js",
  "files": [
    "dist/",
    "loader/",
    "readme.md"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^1.12.2"
  },
  "license": "MIT",
  "dependencies": {
    "feather-icons": "^4.28.0"
  }
}
